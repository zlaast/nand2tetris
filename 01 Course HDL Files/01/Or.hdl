 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=n1);
    Not (in=b, out=n2);
    And (a=n1, b=n2, out=a1);
    Not (in=a1, out=out);
}
