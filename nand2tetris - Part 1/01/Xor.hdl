/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=n1);
    Not (in=b, out=n2);
    And (a=a,  b=n2, out=a1);
    And (a=n1, b=b,  out=a2);
    Or  (a=a1, b=a2, out=out);
}